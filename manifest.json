{
  "name": "TaxPro",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "downloads"
  ],
  "background": {
    "service_worker": "./dist/background.bundle.js",
    "type": "module"
  },
  "action": {
    "default_popup": "main.html",
    "default_title": "TaxPro Extension",
    "default_icon": "./assets/FinAc_simvol.png"
  },
  "icons": {
    "16": "./assets/FinAc_simvol.png",
    "48": "./assets/FinAc_simvol.png",
    "128": "./assets/FinAc_simvol.png"
  },
"host_permissions": ["https://new.e-taxes.gov.az/*"]

}
