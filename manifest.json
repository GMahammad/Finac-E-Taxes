{
  "name": "TaxPro",
  "version": "1.0.2",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "downloads"
  ],
  "background": {
    "service_worker": "./dist/background.bundle.js",
    "type": "module"
  },
  "action": {
    "default_popup": "main.html",
    "default_title": "TaxPro Extension",
    "default_icon": "assets/companysymbol.png"
  },
  "icons": {
    "16": "assets/companysymbol.png",
    "48": "assets/companysymbol.png",
    "128": "assets/companysymbol.png"
  },
"web_accessible_resources": [
  {
    "resources": ["assets/files/example.xlsx"],
    "matches": ["<all_urls>"]
  }
]
}
